{
  "name": "glimpse",
  "version": "1.0",
  "description": "Visual history and tab manager",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "TODO": "only get the necessary permissions",
  "permissions": ["activeTab", "tabs", "<all_urls>", "tabCapture", "webNavigation"],
  "browser_action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
