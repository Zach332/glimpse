{
  "name": "glimpse",
  "version": "1.0",
  "description": "Visual history and tab manager",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "permissions": ["<all_urls>", "tabs", "webNavigation", "bookmarks"],
  "browser_action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
