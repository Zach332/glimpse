{
  "name": "glimpse",
  "version": "0.0.0.1",
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "description": "Visual tab manager",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "permissions": ["<all_urls>", "activeTab", "tabs", "webNavigation", "bookmarks"],
  "browser_action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
